{
	"name": "back-end",
	"version": "1.0.0",
	"main": "index.js",
	"license": "MIT",
	"scripts": {
		"start": "node -r dotenv/config bin/www",
		"dev": "nodemon -r dotenv/config bin/www",
		"db:create": "npx sequelize-cli db:create",
		"db:drop": "sequelize db:drop",
		"db:migrate": "npx sequelize-cli db:migrate",
		"db:migrate:fresh": "npx sequelize-cli db:migrate:undo:all",
		"db:seed": "npx sequelize-cli db:seed:all",
		"db:seed:fresh": "npx sequelize-cli db:seed:undo:all",
		"db:migrate:seed": "npm run db:migrate && npm run db:seed",
		"db:migrate:fresh:seed": "npm run db:migrate:fresh && npm run db:migrate:seed",
		"test": "jest"
	},
	"dependencies": {
		"bcryptjs": "^2.4.3",
		"body-parser": "^1.20.2",
		"cors": "^2.8.5",
		"dayjs": "^1.11.2",
		"dotenv": "^16.1.1",
		"express": "^4.18.1",
		"jsonwebtoken": "^8.5.1",
		"morgan": "^1.10.0",
		"pg": "^8.7.3",
		"pg-hstore": "^2.3.4",
		"sequelize": "^6.20.1",
		"sequelize-cli": "^6.4.1",
		"swagger-ui-express": "^4.4.0"
	},
	"devDependencies": {
		"eslint": "^8.41.0",
		"jest": "^29.5.0",
		"nodemon": "^2.0.16",
		"supertest": "^6.3.3"
	}
}
